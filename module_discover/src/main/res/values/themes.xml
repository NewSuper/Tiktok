<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Discover.Theme.AITDBlock" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/discover_purple_500</item>
        <item name="colorPrimaryVariant">@color/discover_purple_700</item>
        <item name="colorOnPrimary">@color/discover_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/discover_teal_200</item>
        <item name="colorSecondaryVariant">@color/discover_teal_700</item>
        <item name="colorOnSecondary">@color/discover_black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Discover.Theme.AITDBlock.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Discover.Theme.AITDBlock.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Discover.Theme.AITDBlock.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>