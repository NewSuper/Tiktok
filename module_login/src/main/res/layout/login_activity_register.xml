<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/img_fp_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dip"
            android:paddingTop="20dp"
            android:onClick="viewClick"
            android:paddingRight="20dp"
            android:paddingBottom="20dp"
            android:src="@drawable/common_ic_black_close" />

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="29dp"
                    android:enabled="true"
                    android:gravity="center"
                    android:text="@string/reg_email_register"
                    android:textColor="@color/login_color_blue_gray"
                    android:textSize="18sp"
                    android:textStyle="bold" />
                <!--切换tab-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="29dp"
                    android:layout_marginRight="30dp"
                    android:gravity="center"
                    android:visibility="gone">

                    <LinearLayout
                        android:id="@+id/ly_register_tab_phone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:onClick="viewClick"
                        android:gravity="right"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_register_tab_phone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:enabled="false"
                                android:gravity="center"
                                android:text="@string/reg_mobile_register"
                                android:textColor="@color/login_color_blue_gray"
                                android:textSize="16sp" />

                            <View
                                android:id="@+id/v_register_tab_phone_line"
                                android:layout_width="31dp"
                                android:layout_height="2dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/shape_5083fc_10dp" />
                        </LinearLayout>
                    </LinearLayout>

                    <View
                        android:layout_width="83dp"
                        android:layout_height="20dp" />

                    <LinearLayout
                        android:id="@+id/ly_register_tab_email"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:onClick="viewClick"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_register_tab_email"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:enabled="true"
                                android:gravity="center"
                                android:text="@string/reg_email_register"
                                android:textColor="@color/login_color_blue_gray"
                                android:textSize="16sp" />

                            <View
                                android:id="@+id/v_register_tab_email_line"
                                android:layout_width="31dp"
                                android:layout_height="2dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/shape_5083fc_10dp"
                                android:visibility="invisible" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <!--手机号-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="78dp"
                    android:layout_marginRight="30dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_select_mobile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:drawableRight="@mipmap/login_drop_icon"
                        android:drawablePadding="4dp"
                        android:paddingRight="5dp"
                        android:text="+65"
                        android:onClick="viewClick"
                        android:textColor="#333333"
                        android:textSize="15sp"
                        android:visibility="gone" />

                    <com.aitd.module_login.widget.FloatLabeledEditText
                        android:id="@+id/edt_reg_mobile_float"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:visibility="gone"
                        app:hintText="@string/reg_mobile">

                        <EditText
                            android:id="@+id/edt_reg_mobile"
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:layout_gravity="bottom"
                            android:layout_marginRight="30dp"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="bottom"
                            android:hint="@string/reg_phone_number_hit"
                            android:inputType="number"
                            android:maxLength="11"
                            android:textColor="#333333"
                            android:textColorHint="#CCCCCC"
                            android:textCursorDrawable="@drawable/shape_87aaff_2dp"
                            android:textSize="15sp" />
                    </com.aitd.module_login.widget.FloatLabeledEditText>

                    <com.aitd.module_login.widget.FloatLabeledEditText
                        android:id="@+id/edt_reg_email_float"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        app:hintText="@string/reg_emial">

                        <EditText
                            android:id="@+id/edt_reg_email"
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:layout_marginRight="30dp"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="bottom"
                            android:hint="@string/reg_email_number_hit"
                            android:inputType="textEmailAddress"
                            android:maxLength="50"
                            android:textColor="#333333"
                            android:textColorHint="#CCCCCC"
                            android:textCursorDrawable="@drawable/shape_87aaff_2dp"
                            android:textSize="15sp" />
                    </com.aitd.module_login.widget.FloatLabeledEditText>

                    <ImageView
                        android:id="@+id/img_reg_mobile_or_email_close"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="25dp"
                        android:onClick="viewClick"
                        android:src="@mipmap/login_ic_huati_icon_shousuo_ssk"
                        android:visibility="gone" />
                </LinearLayout>

                <View
                    android:id="@+id/v_under_line_mobile_or_email"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginRight="30dp"
                    android:background="#E9E9E9" />

                <LinearLayout
                    android:id="@+id/ly_reg_emial_show"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="9dp"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/tv_matcher_contains_emial"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:text="@string/email_format_error"
                        android:textColor="#F26353"
                        android:textSize="12sp" />
                </LinearLayout>
                <!--输入验证码-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:layout_marginRight="30dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <com.aitd.module_login.widget.FloatLabeledEditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginRight="30dp"
                        android:layout_weight="1"
                        app:hintText="@string/reg_verification_code">

                        <EditText
                            android:id="@+id/edt_reg_code"
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:layout_weight="1"
                            android:background="@null"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:gravity="bottom"
                            android:hint="@string/reg_verification_code_hit"
                            android:inputType="number"
                            android:maxLength="10"
                            android:textColor="#333333"
                            android:textColorHint="#CCCCCC"
                            android:textCursorDrawable="@drawable/shape_87aaff_2dp"
                            android:textSize="15sp" />
                    </com.aitd.module_login.widget.FloatLabeledEditText>

                    <ImageView
                        android:id="@+id/img_reg_code_close"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_marginRight="20dp"
                        android:onClick="viewClick"
                        android:background="@mipmap/login_ic_huati_icon_shousuo_ssk"
                        android:visibility="gone" />

                    <View
                        android:layout_width="1dp"
                        android:layout_height="25dp"
                        android:layout_marginTop="@dimen/dp_10"
                        android:background="#CCCCCC" />

                    <TextView
                        android:id="@+id/tv_reg_send_code"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_marginLeft="9dp"
                        android:gravity="center"
                        android:onClick="viewClick"
                        android:maxWidth="150dp"
                        android:text="@string/reg_get_verification_code"
                        android:textColor="@color/login_color_cc_blue" />
                </LinearLayout>

                <View
                    android:id="@+id/v_under_line_code"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginRight="30dp"
                    android:background="#E9E9E9" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:layout_marginRight="30dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <com.aitd.module_login.widget.FloatLabeledEditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginRight="30dp"
                        android:layout_weight="1"
                        app:hintText="@string/reg_set_pwd">

                        <EditText
                            android:id="@+id/edt_reg_set_pwd"
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:layout_weight="1"
                            android:background="@null"
                            android:digits="0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ@#$%^*"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:gravity="bottom"
                            android:hint="@string/reg_set_pwd_hit"
                            android:maxLength="12"
                            android:textColor="#333333"
                            android:textColorHint="#CCCCCC"
                            android:textCursorDrawable="@drawable/shape_87aaff_2dp"
                            android:textSize="15sp" />
                    </com.aitd.module_login.widget.FloatLabeledEditText>

                    <ImageView
                        android:id="@+id/img_reg_set_pwd_close"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:onClick="viewClick"
                        android:background="@mipmap/login_ic_huati_icon_shousuo_ssk"
                        android:visibility="gone" />

                    <CheckBox
                        android:id="@+id/cb_reg_set_pwd_hide_show"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="9dp"
                        android:button="@null"
                        android:checked="false"
                        android:drawableLeft="@drawable/selector_eyes" />
                </LinearLayout>

                <View
                    android:id="@+id/v_under_line_set_pwd"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginRight="30dp"
                    android:background="#E9E9E9" />
                <!--提示语-->
                <LinearLayout
                    android:id="@+id/ly_reg_pwd_show"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="9dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/fp_please_use"
                            android:textColor="#666666"
                            android:textSize="13sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="9dp">

                        <ImageView
                            android:id="@+id/img_matcher_contains_one"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:src="@mipmap/login_ic_check_fail" />

                        <TextView
                            android:id="@+id/tv_matcher_contains_one"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="7dp"
                            android:text="@string/fp_please_hit_one"
                            android:textColor="#666666"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="8dp">

                        <ImageView
                            android:id="@+id/img_matcher_contains_two"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:src="@mipmap/login_ic_check_fail" />

                        <TextView
                            android:id="@+id/tv_matcher_contains_two"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="7dp"
                            android:text="@string/fp_please_hit_two"
                            android:textColor="#666666"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="8dp">

                        <ImageView
                            android:id="@+id/img_matcher_contains_three"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:src="@mipmap/login_ic_check_fail" />

                        <TextView
                            android:id="@+id/tv_matcher_contains_three"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="7dp"
                            android:text="@string/fp_please_hit_three"
                            android:textColor="#666666"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="8dp">

                        <ImageView
                            android:id="@+id/img_matcher_contains_four"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:src="@mipmap/login_ic_check_fail" />

                        <TextView
                            android:id="@+id/tv_matcher_contains_four"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="7dp"
                            android:text="@string/fp_please_hit_four"
                            android:textColor="#666666"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>
                <!--设置昵称-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginRight="30dp"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <com.aitd.module_login.widget.FloatLabeledEditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        app:hintText="@string/reg_set_niname">

                        <EditText
                            android:id="@+id/edt_reg_niname"
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:layout_marginRight="30dp"
                            android:layout_weight="1"
                            android:background="@null"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:gravity="bottom"
                            android:hint="@string/reg_set_niname_hit"
                            android:maxLength="12"
                            android:textColor="#333333"
                            android:textColorHint="#CCCCCC"
                            android:textCursorDrawable="@drawable/shape_87aaff_2dp"
                            android:textSize="15sp" />
                    </com.aitd.module_login.widget.FloatLabeledEditText>

                    <ImageView
                        android:id="@+id/img_reg_set_niname_close"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:onClick="viewClick"
                        android:background="@mipmap/login_ic_huati_icon_shousuo_ssk"
                        android:visibility="gone" />
                </LinearLayout>

                <View
                    android:id="@+id/v_under_line_set_niname"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginRight="30dp"
                    android:background="#E9E9E9"
                    android:visibility="gone" />

                <LinearLayout
                    android:id="@+id/ly_reg_niname_show"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="9dp"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/fp_please_use"
                            android:textColor="#666666"
                            android:textSize="13sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="8dp">

                        <ImageView
                            android:id="@+id/img_matcher_contains_niname"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:src="@mipmap/login_ic_check_fail" />

                        <TextView
                            android:id="@+id/tv_matcher_contains_niname"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="7dp"
                            android:text="@string/reg_one_ten_font_hit"
                            android:textColor="#666666"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>

                <!--设置邀请码-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginRight="30dp"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <com.aitd.module_login.widget.FloatLabeledEditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        app:hintText="@string/reg_invitaion_code">

                        <EditText
                            android:id="@+id/edt_reg_invitation_code"
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:layout_marginRight="30dp"
                            android:layout_weight="1"
                            android:background="@null"
                            android:digits="0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:gravity="bottom"
                            android:hint="@string/reg_invitaion_code_hit"
                            android:maxLength="12"
                            android:textColor="#333333"
                            android:textColorHint="#CCCCCC"
                            android:textCursorDrawable="@drawable/shape_87aaff_2dp"
                            android:textSize="15sp" />
                    </com.aitd.module_login.widget.FloatLabeledEditText>

                    <ImageView
                        android:id="@+id/img_reg_invitation_code_close"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:onClick="viewClick"
                        android:layout_marginTop="@dimen/dp_10"
                        android:background="@mipmap/login_ic_huati_icon_shousuo_ssk"
                        android:visibility="gone" />
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="30dp"
                    android:gravity="left"
                    android:text="@string/reg_invitaion_code"
                    android:textColor="#FFCCCCCC"
                    android:textSize="12sp" />


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="30dp">

                    <RelativeLayout
                        android:id="@+id/rl_input_invatation"
                        android:layout_width="match_parent"
                        android:onClick="viewClick"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/txt_click_input"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:gravity="left"
                            android:text="@string/invitaion_code_click"
                            android:textColor="#FF5083FC"
                            android:textSize="14sp" />

                        <ImageView
                            android:id="@+id/iv_sao_yi_sao"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_gravity="center_vertical"
                            android:padding="5dp"
                            android:onClick="viewClick"
                            android:src="@mipmap/login_ic_scan_code" />
                    </RelativeLayout>


                    <LinearLayout
                        android:id="@+id/ll_invatation_info"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/txt_invitation_code"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:textColor="#333333"
                            android:textSize="15sp" />

                        <LinearLayout
                            android:id="@+id/ll_invitees_info"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            tools:visibility="visible">

                            <View
                                android:layout_width="1dp"
                                android:layout_height="21dp"
                                android:background="#999999" />

                            <ImageView
                                android:id="@+id/iv_invitees_header"
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_marginLeft="16dp"
                                tools:background="@color/common_black" />

                            <TextView
                                android:id="@+id/iv_invitees_name"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="8dp"
                                android:layout_marginRight="30dp"
                                android:gravity="left"
                                android:textColor="#FF333333"
                                android:textSize="14sp"
                                tools:text="某某" />
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/iv_change_invitation"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="5dp"
                            android:onClick="viewClick"
                            android:src="@mipmap/login_ic_change_invitation" />
                    </LinearLayout>
                </RelativeLayout>

                <View
                    android:id="@+id/v_under_line_invitation_code"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginRight="30dp"
                    android:background="#E9E9E9" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="55dp"
                    android:gravity="center">

                    <CheckBox
                        android:id="@+id/ch_register"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="4dp"
                        android:button="@null"
                        android:checked="true"
                        android:drawableLeft="@drawable/selector_choose" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="12sp"
                        android:textColor="@color/common_text_gray_66"
                        android:text="@string/agreement" />

                    <TextView
                        android:id="@+id/tv_reg_agreement"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="3dp"
                        android:onClick="viewClick"
                        android:text="@string/user_argment"
                        android:textColor="#5B6A91"
                        android:textSize="11dp"
                        android:textStyle="bold" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tv_regist_submit"
                    android:layout_width="match_parent"
                    android:layout_height="44dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="12dp"
                    android:onClick="viewClick"
                    android:layout_marginRight="30dp"
                    android:layout_marginBottom="20dp"
                    android:background="@drawable/selector_blue"
                    android:enabled="false"
                    android:gravity="center"
                    android:text="@string/reg_sure_submit"
                    android:textColor="@color/common_white"
                    android:textSize="15sp" />
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>