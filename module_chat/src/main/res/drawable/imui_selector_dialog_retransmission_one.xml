<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <corners android:topLeftRadius="@dimen/dp_15" android:topRightRadius="@dimen/dp_15" />
            <solid android:color="@color/dialog_btn_pressed" />
            <padding android:bottom="@dimen/dp_15" android:top="@dimen/dp_15" />
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape>
            <corners android:topLeftRadius="@dimen/dp_15" android:topRightRadius="@dimen/dp_15" />
            <solid android:color="@color/dialog_btn_normal" />
            <padding android:bottom="@dimen/dp_15" android:top="@dimen/dp_15" />
        </shape>
    </item>
</selector>